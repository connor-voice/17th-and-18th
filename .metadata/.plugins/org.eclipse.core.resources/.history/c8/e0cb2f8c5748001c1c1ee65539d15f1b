package domain;

import com.jdbctest.database.DogDBFunctions;

public class Runner {

	public static void main(String[] args) {
	
		Dog fiftySix = new Dog("connor but new","sausage",21,30.435f);
		
		DogDBFunctions test = new DogDBFunctions();
		
		test.create(new Dog("connor","sausage",20,30.456f));
		test.create(new Dog("jordan","rotweiler",20,30.456f));
		test.create(new Dog("smith","pomeranian",20,30.456f));
		test.create(new Dog("barry","golden retreiver",20,30.456f));
		test.create(new Dog("broken dog","chihuahua",20,30.456f));
		test.create(new Dog("toby","big good boi",20,30.456f));
		test.create(new Dog("gunther","bulldog",20,30.456f));
		test.create(new Dog("stitch","pug",20,30.456f));
		test.create(new Dog("fluffy","yorkshire terrier",20,30.456f));
		test.create(new Dog("molly","staffordshire bull terrier",20,30.456f));
		
		test.delete(5);
		
//		test.update(1, fiftySix);
	}
	
}
